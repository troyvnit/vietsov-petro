<!DOCTYPE html>
<html lang="en">
    <head>
        <meta name="viewport" content="width=device-width" />
        <title>@ViewBag.Title</title>
        <!---Kendo CSS--->
        <link href="@Url.Content("~/KendoUI/content/shared/styles/examples-offline.css")" rel="stylesheet" />
        <link href="@Url.Content("~/KendoUI/styles/kendo.common.min.css")" rel="stylesheet" />
        <link href="@Url.Content("~/KendoUI/styles/kendo.metroblack.min.css")" rel="stylesheet" type="text/css" />
